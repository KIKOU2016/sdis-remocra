    <entry>
      <name>CREATE TABLE remocra_sgo.hydrant_export_sgo</name>
      <description/>
      <type>SQL</type>
      <sql>CREATE TABLE remocra_sgo.hydrant_export_sgo AS 
SELECT 
    h.numero,
    thn.nom AS type,
    thd.nom as domaine,
    h.voie,
    h.voie2,
    c.insee as insee_commune,
    c.nom as commune,
    hp.debit,
    COALESCE(
      (SELECT avg(debit)::integer AS moyenne_debit
      FROM 
        (SELECT DISTINCT ON (date_contr, id_hydrant) id_hydrant, debit
        FROM tracabilite.hydrant
        WHERE id_hydrant = h.id
         AND date_contr IS NOT NULL
        ORDER BY date_contr DESC
        LIMIT 3) avg_debit),
      null
    ) AS moyenne_debit,
    st_x(h.geometrie) AS coord_x,
    st_y(h.geometrie) AS coord_y,
    dispo_terrestre AS disponibilite
FROM remocra.hydrant as h
INNER JOIN remocra.type_hydrant_nature as thn ON (thn.id = h.nature)
INNER JOIN remocra.type_hydrant_domaine as thd ON (thd.id = h.domaine)
INNER JOIN remocra.commune as c ON (c.id=h.commune)
LEFT JOIN remocra.hydrant_pibi as hp ON (hp.id=h.id);</sql>
