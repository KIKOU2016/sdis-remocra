    <entry>
      <name>Calcul le nb de ligne différente</name>
      <description/>
      <type>EVAL_TABLE_CONTENT</type>
      <connection>Remocra (Postgis)</connection>
      <schemaname/>
      <tablename/>
      <success_condition>rows_count_different</success_condition>
      <limit>0</limit>
      <is_custom_sql>Y</is_custom_sql>
      <is_usevars>N</is_usevars>
      <custom_sql>SELECT NULL FROM generate_series(1, 
  (SELECT count(0) as nb_diff
FROM remocra.hydrant as h
INNER JOIN remocra.type_hydrant_nature as thn ON (thn.id = h.nature)
INNER JOIN remocra.type_hydrant_domaine as thd ON (thd.id = h.domaine)
INNER JOIN remocra.commune as c ON (c.id=h.commune)
LEFT JOIN remocra.hydrant_pibi as hp ON (hp.id=h.id)
LEFT JOIN remocra_sgo.hydrant_export_sgo as hes ON (hes.numero = h.numero)

WHERE (COALESCE(h.numero,&apos;&apos;) &lt;&gt; COALESCE(hes.numero,&apos;&apos;)
	OR COALESCE(thn.nom,&apos;&apos;) &lt;&gt; COALESCE(hes.type,&apos;&apos;)
	OR COALESCE(thd.nom,&apos;&apos;) &lt;&gt; COALESCE(hes.domaine,&apos;&apos;)
	OR COALESCE(h.voie,&apos;&apos;) &lt;&gt; COALESCE(hes.voie,&apos;&apos;)
	OR COALESCE(h.voie2,&apos;&apos;) &lt;&gt; COALESCE(hes.voie2,&apos;&apos;)
	OR c.insee &lt;&gt; hes.insee_commune
	OR COALESCE(hp.debit,0) &lt;&gt; COALESCE(hes.debit,0)
	OR COALESCE(
	      (SELECT avg(debit)::integer AS moyenne_debit
	      FROM 
		(SELECT DISTINCT ON (date_contr, id_hydrant) id_hydrant, debit
		FROM tracabilite.hydrant
		WHERE id_hydrant = h.id
		 AND date_contr IS NOT NULL
		ORDER BY date_contr DESC
		LIMIT 3) avg_debit),
	      0
	    ) &lt;&gt; hes.moyenne_debit
	OR st_x(h.geometrie) &lt;&gt; hes.coord_x
	OR st_y(h.geometrie) &lt;&gt; hes.coord_y
	OR h.dispo_terrestre &lt;&gt; hes.disponibilite)));

</custom_sql>
