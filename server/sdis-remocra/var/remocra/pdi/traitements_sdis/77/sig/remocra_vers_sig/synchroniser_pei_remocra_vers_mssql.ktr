  <step>
    <name>selection_pei</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>REMOCRA_POSTGIS</connection>
    <sql>SELECT h.id, substr(h.numero,1,10) AS IdPtEau, h.numero_interne AS NUM_PEI,
   substr(hn.code_nature_sgo,1,80) AS NATURE,
   substr(thdo.code,1,20) AS STATUT, substr(c.insee,1,5) AS CODE_INSEE, c.nom AS COMMUNE,
   substr(carroy.idcarre200,1,10) AS CARROYAGE, h.dispo_terrestre, traca.date_operation AS created_date, 
   traca.auteur_modification AS created_user, h.date_modification AS last_edited_date,
   COALESCE(u.nom,&apos; &apos;) || &apos; &apos; || COALESCE(u.prenom, &apos; &apos;) AS last_edited_user, now() AS Date_Extraction
FROM remocra.hydrant h
   INNER JOIN remocra.hydrant_pibi hpi ON hpi.id = h.id
   LEFT JOIN remocra.commune c ON c.id = h.commune
   LEFT JOIN remocra.type_hydrant_domaine thdo ON thdo.id = h.domaine
   LEFT JOIN remocra.type_hydrant_nature thn ON thn.id = h.nature
   LEFT JOIN remocra.type_hydrant_diametre thdi ON thdi.id = hpi.diametre
   LEFT JOIN remocra_sgo.hydrant_nature hn ON hn.code_nature_remocra = COALESCE(thn.code, &apos; &apos;) || &apos;_&apos; || COALESCE(thdi.code, &apos; &apos;)
   LEFT JOIN (SELECT id_hydrant, date_operation, auteur_modification FROM tracabilite.hydrant WHERE nom_operation = &apos;INSERT&apos;) AS traca ON traca.id_hydrant = h.id
   LEFT JOIN (SELECT idcarre200, shape FROM remocra_referentiel.carre_200) carroy ON ST_Contains(carroy.shape, h.geometrie)
   LEFT JOIN remocra.utilisateur u ON u.id = h.utilisateur_modification

UNION

SELECT h.id, substr(h.numero,1,10) AS IdPtEau, h.numero_interne AS NUM_PEI,
   substr(hn.code_nature_sgo,1,80) AS NATURE,
   substr(thdo.code,1,20) AS STATUT, substr(c.insee,1,5) AS CODE_INSEE, c.nom AS COMMUNE,
   substr(carroy.idcarre200,1,10) AS CARROYAGE, h.dispo_terrestre, traca.date_operation AS created_date, 
   traca.auteur_modification AS created_user, h.date_modification AS last_edited_date,
   COALESCE(u.nom,&apos; &apos;) || &apos; &apos; || COALESCE(u.prenom, &apos; &apos;) AS last_edited_user, now() AS Date_Extraction
FROM remocra.hydrant h
   INNER JOIN remocra.hydrant_pena hpe ON hpe.id = h.id
   LEFT JOIN remocra.commune c ON c.id = h.commune
   LEFT JOIN remocra.type_hydrant_domaine thdo ON thdo.id = h.domaine
   LEFT JOIN remocra.type_hydrant_nature thn ON thn.id = h.nature
   LEFT JOIN remocra.type_hydrant_positionnement thp ON thp.id = hpe.positionnement
   LEFT JOIN remocra_sgo.hydrant_nature hn ON hn.code_nature_remocra = COALESCE(thn.code, &apos; &apos;) || &apos;_&apos; || COALESCE(thp.code, &apos; &apos;)
   LEFT JOIN (SELECT id_hydrant, date_operation, auteur_modification FROM tracabilite.hydrant WHERE nom_operation = &apos;INSERT&apos;) AS traca ON traca.id_hydrant = h.id
