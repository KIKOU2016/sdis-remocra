  <step>
    <name>Colonnes SIG SQL</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>SDIS</connection>
    <sql>SELECT                                          
  &apos;CREATE TABLE remocra_referentiel.&apos; || relname || E&apos;(&apos; ||
  array_to_string(
    array_agg(
      &apos;    &apos; || column_name || &apos; &apos; ||  type || &apos; &apos;|| not_null
    )
    , E&apos;,&apos;
  ) || E&apos;);&apos; as sql_query_create
from
(
  SELECT n.nspname,
    c.relname, a.attname AS column_name,
    pg_catalog.format_type(a.atttypid, a.atttypmod) as type,
    case 
      when a.attnotnull
    then &apos;NOT NULL&apos; 
    else &apos;NULL&apos; 
    END as not_null 
  FROM pg_class c,
   pg_attribute a,
   pg_type t,
   pg_namespace n
   WHERE a.attnum &gt; 0
   AND a.attrelid = c.oid
   AND a.atttypid = t.oid
   AND c.relnamespace = n.oid
   AND n.nspname || &apos;.&apos; || c.relname = &apos;${NOM_SCHEMA}.${NOM_TABLE}&apos;
 ORDER BY a.attnum
) as tabledefinition
group by relname;</sql>
